{
    "name": "ameotoko/contao-dc-sortable",
    "type": "contao-bundle",
    "description": "Contao 5 DataContainer driver with drag'n'drop sorting support",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Andrey Vinichenko",
            "homepage": "https://github.com/ameotoko"
        }
    ],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.2",
        "symfony/config": "^6.3",
        "symfony/dependency-injection": "^6.3",
        "symfony/http-kernel": "^6.3"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3.1"
    },
    "extra": {
        "contao-manager-plugin": "Ameotoko\\DCSortableBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Ameotoko\\DCSortableBundle\\": "src/"
        }
    }
}
